S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET_NAME=marketplace-photos
S3_REGION=us-east-1
S3_PUBLIC_URL_BASE=http://localhost:9000

